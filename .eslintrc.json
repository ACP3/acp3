{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true,
    "es2022": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "globals": {
    "grecaptcha": false,
    "Cookies": false
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2022
  },
  "rules": {
    "no-dupe-class-members": "off",
    "no-unused-vars": [
      "error",
      {
        "vars": "local"
      }
    ],
    "no-var": "error",
    "no-console": [
      "error",
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
