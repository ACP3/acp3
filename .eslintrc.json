{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier", "prettier/babel"],
  "globals": {
    "grecaptcha": false,
    "bootbox": false,
    "Cookies": false
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["@babel"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "local"
      }
    ],
    "no-var": "error",
    "no-console": [
      "error",
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
