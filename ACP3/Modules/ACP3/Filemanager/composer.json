{
  "name": "acp3/module-filemanager",
  "description": "Provides the filemanager module for the ACP3 CMS.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Tino Goratsch",
      "email": "mail@acp3-cms.net"
    }
  ],
  "support": {
    "email": "mail@acp3-cms.net",
    "issues": "https://gitlab.com/ACP3/cms/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "acp3/core": "^6.21.0",
    "acp3/module-permissions": "^6.18.0",
    "acp3/module-system": "^6.21.0",
    "illuminate/config": "^10.0",
    "illuminate/container": "^10.0",
    "illuminate/support": "^10.0",
    "symfony/http-foundation": "^6.4|^7.0",
    "symfony/event-dispatcher": "^6.4|^7.0"
  },
  "suggest": {
    "acp3/setup": "Allows you to comfortably install and update the ACP3 components via an UI."
  },
  "autoload": {
    "psr-4": {
      "ACP3\\Modules\\ACP3\\Filemanager\\": "src",
      "RFM\\": "libs/RFM"
    },
    "files": [
      "registration.php",
      "libs/RFM/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ACP3\\Modules\\ACP3\\Filemanager\\": "tests"
    }
  }
}
