{
  "name": "acp3/module-search",
  "description": "Provides the search module for the ACP3 CMS.",
  "type": "acp3-module",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Tino Goratsch",
      "email": "mail@acp3-cms.net"
    }
  ],
  "support": {
    "email": "mail@acp3-cms.net",
    "issues": "https://github.com/ACP3/cms/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "acp3/composer-installer": "*",
    "acp3/core": "4.1.6",
    "acp3/setup": "4.1.6",
    "acp3/module-errors": "4.1.6",
    "acp3/module-permissions": "4.1.6",
    "acp3/module-seo": "4.1.6",
    "acp3/module-system": "4.1.6",
    "acp3/module-users": "4.1.6"
  },
  "autoload": {
    "psr-4": {
      "ACP3\\Modules\\ACP3\\Search\\": ""
    }
  },
  "suggest": {
    "acp3/module-articles": "Makes it possible to search through Articles.",
    "acp3/module-files": "Makes it possible to search through the Downloads.",
    "acp3/module-news": "Makes it possible to search through the News."
  },
  "extra": {
    "installer-name": "ACP3/Search"
  }
}
