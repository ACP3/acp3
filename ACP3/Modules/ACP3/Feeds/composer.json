{
  "name": "acp3/module-feeds",
  "description": "Provides the feeds module for the ACP3 CMS.",
  "license": "GPL-2.0+",
  "type": "acp3-module",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "acp3/composer-installer": "*",
    "acp3/core": "4.0.0-rc.29",
    "acp3/setup": "4.0.0-rc.29",
    "acp3/module-errors": "4.0.0-rc.29",
    "acp3/module-permissions": "4.0.0-rc.29",
    "acp3/module-seo": "4.0.0-rc.29",
    "acp3/module-system": "4.0.0-rc.29",
    "acp3/module-users": "4.0.0-rc.29",
    "mibe/feedwriter": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ACP3\\Modules\\ACP3\\Feeds\\": ""
    }
  },
  "suggest": {
    "acp3/module-files": "Makes it possible to show feeds for the Downloads module.",
    "acp3/module-news": "Makes it possible to show feeds for the News module."
  },
  "extra": {
    "installer-name": "ACP3/Feeds"
  }
}
