{
  "name": "@acp3/module-system",
  "description": "Provides the system module for the ACP3 CMS.",
  "scripts": {
    "build": "gulp",
    "css-lint": "stylelint \"**/*.{css,scss}\" --cache --cache-location .stylelintcache --rd",
    "css-lint:fix": "npm run css-lint -- --fix",
    "eslint": "eslint -c .eslintrc.json --cache --cache-location=.eslintcache .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check --cache **/*.{js,cjs,mjs,json,md,yml}",
    "prettier:fix": "prettier --write --cache **/*.{js,cjs,mjs,json,md,yml}",
    "lint-all": "concurrently \"npm:prettier:fix\" \"npm:css-lint:fix\" \"npm:eslint:fix\" --kill-others-on-fail --group",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ACP3/cms.git"
  },
  "author": "Tino Goratsch <mail@acp3-cms.net>",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/ACP3/cms/issues"
  },
  "homepage": "https://www.acp3-cms.net",
  "private": true,
  "devDependencies": {
    "@ronilaukkarinen/gulp-stylelint": "^14.1.2",
    "autoprefixer": "^10.4.18",
    "browserslist": "^4.23.0",
    "concurrently": "^8.2.2",
    "cssnano": "^6.1.0",
    "del": "^7.1.0",
    "esbuild": "^0.20.2",
    "esbuild-loader": "^4.1.0",
    "esbuild-plugin-browserslist": "^0.11.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globby": "^14.0.1",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-change": "^1.0.2",
    "gulp-dependents": "^1.2.5",
    "gulp-eslint-new": "^1.9.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sharp-responsive": "^0.4.1",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sass-embedded": "^1.72.0",
    "semver": "^7.6.0",
    "simple-git": "^3.22.0",
    "stylelint": "^15.11.0",
    "stylelint-config-twbs-bootstrap": "^12.0.0",
    "stylelint-prettier": "^4.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "core-js": "^3.36.0",
    "datatables.net": "^2.0.2",
    "datatables.net-bs5": "^2.0.2",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "photoswipe": "^5.4.3"
  }
}
