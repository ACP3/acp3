{
  "name": "acp3/module-contact",
  "description": "Provides the contact module for the ACP3 CMS.",
  "version": "4.13.0",
  "type": "acp3-module",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Tino Goratsch",
      "email": "mail@acp3-cms.net"
    }
  ],
  "support": {
