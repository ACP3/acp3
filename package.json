{
  "name": "acp3-cms",
  "version": "6.15.1",
  "description": "The ACP3 CMS is a highly customizable and easy to use Web Content Management System based on PHP and MySQL.",
  "main": "",
  "directories": {},
  "scripts": {
    "build": "gulp",
    "css-lint": "stylelint \"**/*.{css,scss}\" --cache --cache-location .stylelintcache --rd",
    "css-lint:fix": "npm run css-lint -- --fix",
    "eslint": "eslint -c .eslintrc.json --cache --cache-location=.eslintcache .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check --cache **/*.{css,js,cjs,mjs,json,md,scss,yml}",
    "prettier:fix": "prettier --write --cache **/*.{css,js,cjs,mjs,json,md,scss,yml}",
    "lint-all": "concurrently \"npm:prettier:fix\" \"npm:css-lint:fix\" \"npm:eslint:fix\" --kill-others-on-fail --group",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ACP3/cms.git"
  },
  "author": "Tino Goratsch <mail@acp3-cms.net>",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/ACP3/cms/issues"
  },
  "homepage": "https://www.acp3-cms.net",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@ronilaukkarinen/gulp-stylelint": "^14.1.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.15",
    "del": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globby": "^13.1.3",
    "gulp": "^4.0.2",
    "gulp-buffer": "0.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-change": "^1.0.2",
    "gulp-dependents": "^1.2.5",
    "gulp-eslint-new": "^1.7.2",
    "gulp-filter": "^7.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sharp-responsive": "^0.4.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tap": "^2.0.0",
    "gulplog": "^2.0.1",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "semver": "^7.3.8",
    "simple-git": "^3.17.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-twbs-bootstrap": "^7.0.0",
    "stylelint-prettier": "^2.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.7.1"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "ckeditor-codemirror-plugin": "^1.18.6",
    "ckeditor4": "^4.20.2",
    "core-js": "^3.29.0",
    "datatables.net": "^1.13.3",
    "datatables.net-bs5": "^1.13.3",
    "jquery": "^3.6.3",
    "js-cookie": "^3.0.1",
    "klaro": "^0.7.18",
    "photoswipe": "^5.3.6",
    "regenerator-runtime": "^0.13.11",
    "rich-filemanager": "https://github.com/tinogo/RichFilemanager/tarball/rework-dependencies",
    "shariff": "^3.2.1",
    "tinymce": "^4.9.11"
  },
  "browserslist": [
    "defaults and >= 1%",
    "Firefox ESR"
  ],
  "lint-staged": {
    "*.{js,cjs,mjs}": "npm run eslint:fix",
    "*.{css,scss}": "npn run css-lint:fix",
    "*.{css,js,cjs,mjs,json,md,scss,yml}": "npm run prettier:fix",
    "*.php": "docker-compose run --no-deps --rm app composer php-cs-fixer"
  }
}
