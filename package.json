    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11.0.4",
    "gulp": "^4.0.2",
    "gulp-buffer": "0.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-change": "^1.0.2",
    "gulp-dependents": "^1.2.5",
    "gulp-eslint-new": "^1.5.1",
    "gulp-filter": "^7.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
