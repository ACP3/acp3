{
  "name": "acp3/cms",
  "description": "ACP3 is a highly customizable and easy to use web content management system based on PHP5 and MySQL.",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Tino Goratsch",
      "email": "mail@acp3-cms.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "ACP3\\": "./ACP3",
      "ACP3\\Installer\\": "./installation/Installer"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.0",
    "doctrine/dbal": "^2.5",
    "doctrine/cache": "^1.6",
    "inlinestyle/inlinestyle": "^1.2",
    "monolog/monolog": "^1.18",
    "mrclay/minify": "^2.2",
    "patchwork/utf8": "^1.3",
    "phpmailer/phpmailer": "^5.2",
    "smarty/smarty": "^3.1",
    "symfony/config": "^2.8",
    "symfony/dependency-injection": "^2.8",
    "symfony/http-foundation": "^2.8",
    "symfony/yaml": "^2.8",
    "symfony/event-dispatcher": "^2.8",
    "mibe/feedwriter": "^1.0",
    "ckeditor/ckeditor": "dev-full/4.5.x",
    "tinymce/tinymce": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "satooshi/php-coveralls": "dev-master",
    "fabpot/php-cs-fixer": "^1.11"
  }
}
